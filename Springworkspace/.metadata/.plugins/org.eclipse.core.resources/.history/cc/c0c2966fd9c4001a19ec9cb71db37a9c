package member;

import java.util.HashMap;

import org.springframework.ui.Model;

public interface MemberService {
	//회원가입시 회원정보 저장
	int member_insert(MemberVO vo);
	//마이페이지에서 회원정보 확인
	MemberVO member_select(String id);
	//로그인처리
	MemberVO member_login(HashMap<String, String> map);
	//아이디 중복확인
	boolean member_id_check(String id);
	//마이페이지에서 회원정보변경저장
	boolean member_update(MemberVO vo);
	//회원정보탈퇴
	boolean member_delete(String id);
	//회원가입시 중복 확인
	int member_join_select(HashMap<String, String> map);
	//SNS 계정으로 로그인
	String member_sns_login(String snsId);
}
